{
    "name": "samheldev/excel-mapper",
    "description": "Laravel package to map Excel columns to database fields",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam",
            "email": "mahmoud.samy7729@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "maatwebsite/excel": "^3.1",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sam\\ExcelMapper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sam\\ExcelMapper\\ExcelMapperServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
